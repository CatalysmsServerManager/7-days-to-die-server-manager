{
  "name": "csmm",
  "version": "1.16.0",
  "description": "A 7dtd server manager made with Sails.js",
  "keywords": [],
  "dependencies": {
    "7daystodie-api-wrapper": "1.5.4",
    "@sailshq/connect-redis": "^3.2.1",
    "@sailshq/socket.io-redis": "^5.2.0",
    "@sentry/apm": "^5.19.2",
    "@sentry/node": "^5.19.2",
    "@streammedev/hhmmss": "^1.0.0",
    "async": "2.6.1",
    "bull": "^3.11.0",
    "bull-board": "^0.9.0",
    "cron-parser": "^2.7.3",
    "cross-env": "^7.0.2",
    "discord.js": "^11.4.2",
    "discord.js-commando": "^0.10.0",
    "dotenv": "^8.2.0",
    "geoip-lite": "^1.4.2",
    "grunt": "1.0.3",
    "he": "^1.2.0",
    "lodash": "^4.17.19",
    "lusca": "^1.6.1",
    "machinepack-7daystodiewebapi": "^0.3.4",
    "minimist": "^1.2.3",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "p-wait-for": "^3.1.0",
    "passport": "^0.4.1",
    "passport-discord": "^0.1.4",
    "passport-steam": "^1.0.14",
    "rand-token": "^0.4.0",
    "redis": "^2.8.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "sails": "^1.1.0",
    "sails-hook-grunt": "^4.0.1",
    "sails-hook-orm": "^2.1.1",
    "sails-hook-sockets": "^1.5.5",
    "sails-mysql": "^1.0.1",
    "sails-redis": "^1.0.0",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "validator": "^10.9.0",
    "winston": "^2.4.3"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production run-p start:*",
    "start:app": "node app.js",
    "start:worker": "node worker.js",
    "wait-for:db": "node ./scripts/wait-for-db.js",
    "test": "mocha test ",
    "lint": "eslint --max-warnings 0 .",
    "dev": "nodemon",
    "debug": "node debug app.js",
    "cover": "nyc npm run test",
    "itemIcons:update": "bash ./scripts/itemIconsUpdate.sh",
    "postversion": "git push && git push --tags",
    "sequelize": "sequelize-cli",
    "db:migrate": "npm run sequelize -- db:migrate",
    "db:migrate:undo": "npm run sequelize -- db:migrate:undo"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/CatalysmsServerManager/7-days-to-die-server-manager.git"
  },
  "author": "Catalysm",
  "license": "",
  "engines": {
    "node": "8"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.2",
    "@types/bull": "^3.10.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.7.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.4.0",
    "faker": "^4.1.0",
    "grunt-bower-task": "^0.5.0",
    "mocha": "^8.1.0",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mochawesome": "^6.1.1",
    "mockdate": "^3.0.2",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
    "sails-disk": "^1.1.2",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "supertest": "^4.0.2"
  },
  "mocha": {
    "timeout": 5000,
    "reporter": "mochawesome",
    "exit": true,
    "recursive": true,
    "reporter-options": [
      "reportDir=coverage/mochawesome",
      "reportFilename=index"
    ],
    "file": [
      "./test/lifecycle.test.js"
    ],
    "watch-files": [
      "api/**/*",
      "assets/**/*s",
      "config/**/*.js",
      "test/**/*.test.js",
      "scripts/**/*s",
      "tasks/**/*s",
      "views/**/*"
    ],
    "watch-ignore": [
      "db-data",
      "redis-data",
      "coverage",
      "logs"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ],
    "exclude": [
      "db-data/**/*",
      "redis-data/**/*",
      "assets/**/*",
      "config/**/*",
      "coverage/**/*",
      "tasks/**/*",
      ".tmp/**/*",
      "views/**/*",
      "test/**/*",
      "migrations/**/*"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
