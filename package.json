{
  "name": "csmm",
  "version": "1.23.0",
  "description": "A 7dtd server manager made with Sails.js",
  "keywords": [],
  "dependencies": {
    "7daystodie-api-wrapper": "1.5.6",
    "@sailshq/connect-redis": "^3.2.1",
    "@sailshq/socket.io-redis": "^5.2.0",
    "@sentry/core": "^6.8.0",
    "@sentry/node": "^6.8.0",
    "@sentry/tracing": "^6.8.0",
    "@streammedev/hhmmss": "^1.0.0",
    "async": "3.2.0",
    "bull": "^3.22.8",
    "bull-board": "^2.1.2",
    "concurrently": "^6.2.0",
    "cron-parser": "^3.5.0",
    "cross-env": "^7.0.3",
    "discord.js": "^12.5.3",
    "discord.js-commando": "^0.12.3",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "geoip-lite": "^1.4.2",
    "grunt": "1.4.1",
    "handlebars": "^4.7.7",
    "he": "^1.2.0",
    "lodash": "^4.17.21",
    "lusca": "^1.7.0",
    "machinepack-7daystodiewebapi": "^0.3.4",
    "minimist": "^1.2.3",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "p-wait-for": "^4.0.0",
    "passport": "^0.4.1",
    "passport-discord": "^0.1.4",
    "passport-steam": "^1.0.15",
    "redis": "^3.1.2",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "safe-regex": "^2.1.1",
    "sails": "^1.4.3",
    "sails-hook-grunt": "^4.0.1",
    "sails-hook-orm": "^3.0.2",
    "sails-hook-sockets": "^2.0.1",
    "sails-mysql": "^1.0.1",
    "sails-redis": "^1.0.0",
    "sequelize": "^6.6.4",
    "sequelize-cli": "^6.2.0",
    "split-string": "^6.1.0",
    "validator": "^13.6.0",
    "winston": "^3.3.3"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production concurrently --kill-others -n 'app,worker' 'npm run start:app' 'npm run start:worker'",
    "start:app": "node app.js",
    "start:worker": "node worker/worker.js",
    "wait-for:db": "node ./scripts/wait-for-db.mjs",
    "test": "mocha test ",
    "lint": "eslint --max-warnings 0 .",
    "dev": "cross-env NODE_ENV=development nodemon --exec concurrently --kill-others -n 'app,worker' 'npm run start:app' 'npm run start:worker'",
    "debug": "node debug app.js",
    "cover": "nyc npm run test",
    "itemIcons:update": "bash ./scripts/itemIconsUpdate.sh",
    "postversion": "git push && git push --tags",
    "sequelize": "sequelize-cli",
    "db:migrate": "npm run sequelize -- db:migrate",
    "db:migrate:undo": "npm run sequelize -- db:migrate:undo"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/CatalysmsServerManager/7-days-to-die-server-manager.git"
  },
  "author": "Catalysm",
  "license": "",
  "engines": {
    "node": "12"
  },
  "devDependencies": {
    "@types/bull": "^3.15.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.8.2",
    "coveralls": "^3.1.0",
    "docker-compose": "^0.23.12",
    "eslint": "^7.29.0",
    "faker": "^5.5.3",
    "grunt-bower-task": "^0.5.0",
    "husky": "^4.3.0",
    "lint-staged": "^11.0.0",
    "mocha": "^9.0.1",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mock-req-res": "^1.2.0",
    "mockdate": "^3.0.5",
    "nodemon": "^2.0.8",
    "nyc": "^15.0.1",
    "sinon": "^11.1.1",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.1.3"
  },
  "mocha": {
    "timeout": 10000,
    "exit": true,
    "recursive": true,
    "reporterOptions": {
      "mochaFile": "./reports/mocha/junit.xml"
    },
    "file": [
      "./test/lifecycle.test.js"
    ],
    "exclude": [
      "test/e2e.js"
    ],
    "watch-files": [
      "api/**/*",
      "assets/**/*s",
      "config/**/*.js",
      "test/**/*.test.js",
      "scripts/**/*s",
      "tasks/**/*s",
      "views/**/*"
    ],
    "watch-ignore": [
      "db-data",
      "redis-data",
      "coverage",
      "logs"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ],
    "exclude": [
      "db-data/**/*",
      "redis-data/**/*",
      "assets/**/*",
      "config/**/*",
      "coverage/**/*",
      "tasks/**/*",
      ".tmp/**/*",
      "views/**/*",
      "test/**/*",
      "migrations/**/*"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./reports/coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "npm run lint"
  }
}
