{
  "name": "csmm",
  "version": "1.11.14",
  "description": "A 7dtd server manager made with Sails.js",
  "keywords": [],
  "dependencies": {
    "7daystodie-api-wrapper": "^1.5.0",
    "@sailshq/connect-redis": "^3.2.1",
    "@sailshq/socket.io-redis": "^5.2.0",
    "@sentry/node": "^4.5.1",
    "@streammedev/hhmmss": "^1.0.0",
    "async": "2.6.1",
    "bull": "^3.11.0",
    "cron-parser": "^2.7.3",
    "discord.js": "^11.4.2",
    "discord.js-commando": "^0.10.0",
    "dotenv": "^6.2.0",
    "geoip-country": "^4.0.4",
    "geoip-ultralight": "^0.1.5",
    "grunt": "1.0.3",
    "he": "^1.2.0",
    "lodash": "^4.17.13",
    "lusca": "^1.6.1",
    "machinepack-7daystodiewebapi": "^0.3.2",
    "minimist": "^1.2.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "node-schedule": "^1.3.1",
    "passport": "^0.4.0",
    "passport-discord": "^0.1.3",
    "passport-steam": "^1.0.10",
    "rand-token": "^0.4.0",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "sails": "^1.1.0",
    "sails-hook-grunt": "^3.1.0",
    "sails-hook-orm": "^2.1.1",
    "sails-hook-sockets": "^1.5.5",
    "sails-mysql": "^1.0.1",
    "sails-redis": "^1.0.0",
    "validator": "^10.9.0",
    "winston": "^2.4.3"
  },
  "scripts": {
    "start": "NODE_ENV=production & node app.js",
    "test": "mocha test",
    "build-docs": "./node_modules/.bin/jsdoc -c ./.jsdoc.json",
    "dev": "nodemon",
    "debug": "node debug app.js",
    "cover": "nyc npm run test",
    "itemIcons:update": "bash ./scripts/itemIconsUpdate.sh"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/CatalysmsServerManager/7-days-to-die-server-manager.git"
  },
  "author": "Catalysm",
  "license": "",
  "engines": {
    "node": "8"
  },
  "devDependencies": {
    "@types/bull": "^3.10.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.1.0",
    "coveralls": "^3.1.0",
    "faker": "^4.1.0",
    "grunt-bower-task": "^0.5.0",
    "mocha": "^7.1.2",
    "mocha-junit-reporter": "^1.23.3",
    "mocha-lcov-reporter": "^1.3.0",
    "mochawesome": "^6.1.1",
    "mockdate": "^2.0.5",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
    "sails-disk": "^1.1.2",
    "supertest": "^3.3.0"
  },
  "mocha": {
    "reporter": "mochawesome",
    "exit": true,
    "recursive": true,
    "reporter-options": [
      "reportDir=coverage/mochawesome",
      "reportFilename=index"
    ],
    "file": [
      "./test/lifecycle.test.js"
    ],
    "watch-files": [
      "api/**/*",
      "assets/**/*s",
      "config/**/*.js",
      "test/**/*.test.js",
      "scripts/**/*s",
      "tasks/**/*s",
      "views/**/*"
    ],
    "watch-ignore": [
      "db-data",
      "redis-data",
      "coverage",
      "logs"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ],
    "exclude": [
      "db-data/**/*",
      "redis-data/**/*",
      "assets/**/*",
      "config/**/*",
      "coverage/**/*",
      "tasks/**/*",
      ".tmp/**/*",
      "views/**/*"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  }
}
