          </div>
        </div>

      </div>

      <!-- Message of the day settings -->

      <div class="card">
        <div class="card-header" id="settings-heading-motdSettings">
          <h5 class="mb-0">
            <button class="btn btn-link" data-toggle="collapse" data-target="#settings-collapse-motdSettings">
              <i class="far fa-comment fa-lg"></i> Message of the day
            </button>
          </h5>
        </div>

        <div id="settings-collapse-motdSettings" class="collapse">
          <div class="card-body">
            <%- include('./partials/settings/motdSettings', {server: server, config: config}) %>

          </div>
        </div>

      </div>

      <div class="card">
        <div class="card-header" id="settings-heading-economySettings">
          <h5 class="mb-0">
            <button class="btn btn-link" data-toggle="collapse" data-target="#settings-collapse-economySettings">
              <i class="fas fa-money-bill fa-lg "></i> Economy
            </button>
          </h5>
        </div>

        <div id="settings-collapse-economySettings" class="collapse">
          <div class="card-body">
